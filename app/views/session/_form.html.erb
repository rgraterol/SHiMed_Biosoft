<%= form_for :session, url: sessions_path, html: {class: "horizontal-form", role:"form"} do |f|  %>
    <div class="form-group">
      <%= f.label :email, :class=>"exampleInputEmail1" %>

      <%= f.text_field :email, :class=>"form-control", :placeholder=>"Ingresa tu Email" %>

    </div>
    <div class="form-group">
      <%= f.label "Contraseña",  :class=>"exampleInputPassword1" %>
      <%= f.password_field :password, :class=>"form-control", :placeholder=>"Ingresa tu Contraseña" %>
    </div>
    <div class="form-group">
      <div class="col-sm-offset-2 col-sm-10">
        <%= f.submit 'Iniciar Sesión', :class=>"btn btn-default" %><br>
      </div>
    </div>

    <p>Nuevo Usuario? <%= link_to "Registrate YA!", new_usuario_path %></p>
<% end %>